<%= button_to"Home Page","/contacts", method: :get %>
<h1>Gideon's Adrees</h1>

<table class= "table table border">
  <tr>
    <th>  First_Name  </th>
    <th>  Middle_Name </th>
    <th>  Last_Name  </th>
    <th>  Full_Name  </th>
    <th>  Bio  </th>
    <th>  Adress  </th>
    <th>  Longitude </th>
    <th>  Latitude  </th>
    <th>  Phone  </th>
  </tr>
  <% @contacts.each do |contact| %>
    <tr>
      <td><%= contact.first_name%></td>
      <td><%= contact.middle_name %></td>
      <td><%= contact.last_name %></td>
      <td><%= link_to contact.full_name,"/contacts/#{contact.id}" %></td>
      <td><%= contact.bio %></td>
      <td><%= contact.adress %></td>
      <td><%= contact.longitude %></td>
      <td><%= contact.latitude %></td>
      <td><%= contact.phone %></td>
   
    </tr>
  <% end %>

</table>


<%= button_to "Add New Contact", "/contacts/new", method: :get %>
