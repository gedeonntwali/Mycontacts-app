<%= button_to"Home Page","/contacts", method: :get %>
<h1> Edit Contact </h1>
<%= form_tag "/contacts/#{@contact.id}", method: :patch do%>
  <div class="form-group">
    First_name: <input type="text" class="form-group" name="first_name" value="<%= @contact.first_name %>">
  </div>
  <div class="form-group">
    Middle_name: <input type="text" class="form-group" name="middle_name" value="<%= @contact.middle_name %>">
  </div>

  <div class="form-group">
    Last_name: <input type="text" class="form-group" name="last_name" value="<%= @contact.last_name %>">
  </div>
  
  <div class="form-group">
    Bio: <input type="text" class="form-group" name="bio">
  </div>

  <div class="form-group">
    Adress: <input type="text" class="form-group" name="adress" value="<%= @contact.adress %>">
  </div>

  <div class="form-group">
    Longitude: <input type="text" class="form-group" name="longitude" value="<%= @contact.longitude %>">
  </div>
  
  <div class="form-group">
    Latitude: <input type="text" class="form-group" name="latitude" value="<%= @contact.latitude %>">
  </div>


  <div class="form-group">
    Phone: <input type="text" class="form-group" name="phone" value="<%= @contact.phone %>">
  </div>

  <div class="checkbox">
    <input type="submit" class="btn btn-default" Value="Edit Contact">
  </div>
<% end %>