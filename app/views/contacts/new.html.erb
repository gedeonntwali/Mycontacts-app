<%= button_to"Home Page","/contacts", method: :get %>
<h1> New Contact </h1>
<%= form_tag "/contacts", method: :post do%>
  <div class="form-group">
    First_name: <input type="text" class="form-group" name="first_name">
  </div>
  <div class="form-group">
    Middle_Name: <input type="text" class="form-group" name="middle_name">
  </div>

  <div class="form-group">
    Last_name: <input type="text" class="form-group" name="last_name">
  </div>
  <div class="form-group">
    Bio: <input type="text" class="form-group" name="bio">
  </div>

  <div class="form-group">
    Adress: <input type="text" class="form-group" name="adress">
  </div>

  <div class="form-group">
    Longitude: <input type="text" class="form-group" name="longitude">
  </div>
  
  <div class="form-group">
    Latitude: <input type="text" class="form-group" name="latitude">
  </div>

  <div class="form-group">
    Phone: <input type="text" class="form-group" name="phone">
  </div>

  <div class="checkbox">
    <input type="submit" class="btn btn-default" name="Create Contact">
  </div>
<% end %>